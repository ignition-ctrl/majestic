#!/usr/bin/env bash
#
# HTML5 Scaffolding
#
# Criado em 04/11/2021 00:11:34
# Atualizado em 04/11/2021 00:30:51
#
# Credits: https://www.taniarascia.com/basic-html5-file/

DIR="basic-html"

test -d ${DIR}/assets/css || \mkdir -p ${DIR}/assets/css
test -f ${DIR}/index.html || curl -s -L https://git.io/JP5mq -o ${DIR}/index.html
test -f ${DIR}/assets/css/main.css || curl -s -L https://git.io/JP5mj -o ${DIR}/assets/css/main.css

code ${DIR}